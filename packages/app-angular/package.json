{
	"name": "app-angular",
	"version": "4.3.1-alpha.1",
	"scripts": {
		"ng": "ng",
		"start": "npm run clean:full && npm i && ng serve",
		"serve": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"clean": "rm -rf build",
		"clean:full": "rm -rf node_modules && npm run clean",
		"prebuild": "npm run copy:assets",
		"copy:images": "copyfiles -E -f \"node_modules/@ongov/ontario-design-system-component-library-angular/dist/component-library/assets/images/**\" src/assets",
		"copy:favicons": "copyfiles -E -f \"node_modules/@ongov/ontario-design-system-component-library-angular/dist/component-library/assets/favicons/**\" src/assets/favicons",
		"copy:fonts": "copyfiles -E -u 6 \"node_modules/@ongov/ontario-design-system-component-library-angular/dist/component-library/assets/fonts/**/*\" src/assets/fonts",
		"copy:assets": "npm run copy:images && npm run copy:favicons && npm run copy:fonts"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^15.2.9",
		"@angular/common": "^15.2.9",
		"@angular/compiler": "^15.2.9",
		"@angular/core": "^15.2.9",
		"@angular/forms": "^15.2.9",
		"@angular/platform-browser": "^15.2.9",
		"@angular/platform-browser-dynamic": "^15.2.9",
		"@angular/router": "^15.2.9",
		"@ngx-translate/core": "^14.0.0",
		"@ngx-translate/http-loader": "^7.0.0",
		"@ongov/ontario-design-system-component-library-angular": "^4.3.1-alpha.1",
		"@ongov/ontario-design-system-design-tokens": "^4.3.1-alpha.1",
		"@ongov/ontario-design-system-global-styles": "^4.3.1-alpha.1",
		"@uirouter/angular": "^11.1.0",
		"copyfiles": "^2.4.1",
		"ngx-translate-routes": "^1.4.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.13.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^15.2.9",
		"@angular/cli": "~15.2.9",
		"@angular/compiler-cli": "^15.2.9",
		"@types/jasmine": "~4.3.0",
		"jasmine-core": "~4.5.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"typescript": "~4.9.4"
	}
}
