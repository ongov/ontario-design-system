{
	"name": "app-angular",
	"version": "7.0.0",
	"type": "module",
	"scripts": {
		"ng": "ng",
		"start": "pnpm run clean:full && pnpm i && ng serve",
		"serve": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"clean": "rm -rf build",
		"clean:full": "rm -rf node_modules && pnpm run clean",
		"prebuild": "pnpm run copy:assets",
		"copy:images": "copyfiles -E -f \"./node_modules/@ongov/ontario-design-system-component-library-angular/dist/component-library/assets/images/**\" src/assets",
		"copy:favicons": "copyfiles -E -f \"./node_modules/@ongov/ontario-design-system-component-library-angular/dist/component-library/assets/favicons/**\" src/assets/favicons",
		"copy:fonts": "copyfiles -E -u 6 \"./node_modules/@ongov/ontario-design-system-component-library-angular/dist/component-library/assets/fonts/**/*\" src/assets/",
		"copy:assets": "pnpm run copy:images && pnpm run copy:favicons && pnpm run copy:fonts"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^21.1.0",
		"@angular/common": "^21.1.0",
		"@angular/compiler": "^21.1.0",
		"@angular/core": "^21.1.0",
		"@angular/forms": "^21.1.0",
		"@angular/platform-browser": "^21.1.0",
		"@angular/platform-browser-dynamic": "^21.1.0",
		"@angular/router": "^21.1.0",
		"@ngx-translate/core": "^14.0.0",
		"@ngx-translate/http-loader": "^7.0.0",
		"@ongov/ontario-design-system-component-library-angular": "workspace:*",
		"@ongov/ontario-design-system-design-tokens": "workspace:*",
		"@ongov/ontario-design-system-global-styles": "workspace:*",
		"@uirouter/angular": "^11.1.0",
		"copyfiles": "^2.4.1",
		"ngx-translate-routes": "^1.4.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^21.1.0",
		"@angular/cli": "^21.1.0",
		"@angular/compiler-cli": "^21.1.0",
		"@types/jasmine": "~6.0.0",
		"jasmine-core": "~6.1.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"typescript": "5.9.3"
	}
}
