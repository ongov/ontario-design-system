{
	"name": "@ongov/ontario-design-system-component-library",
	"version": "6.0.0",
	"type": "module",
	"private": false,
	"description": "Ontario Design System Component Library",
	"license": "Apache-2.0",
	"main": "dist/index.cjs.js",
	"unpkg": "dist/ontario-design-system-components/ontario-design-system-components.esm.js",
	"module": "dist/index.js",
	"types": "dist/types/index.d.ts",
	"files": [
		"components/",
		"dist/",
		"loader/",
		"src/",
		"hydrate/",
		"www/",
		"LICENSE",
		"package.json",
		"readme.md"
	],
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs.js",
			"default": "./dist/index.mjs"
		},
		"./loader": {
			"types": "./loader/index.d.ts",
			"import": "./loader/index.es2017.js",
			"require": "./loader/index.cjs.js",
			"default": "./loader/index.es2017.js"
		},
		"./hydrate": {
			"types": "./hydrate/index.d.ts",
			"import": "./hydrate/index.js",
			"require": "./hydrate/index.cjs.js",
			"default": "./hydrate/index.js"
		},
		"./components/*": {
			"types": "./dist/types/components/*.d.ts",
			"import": "./components/*",
			"require": "./components/*.cjs"
		},
		"./components": {
			"types": "./dist/types/components.d.ts",
			"import": "./components/index.js",
			"require": "./components/index.cjs.js",
			"default": "./components/index.js"
		}
	},
	"scripts": {
		"prebuild": "pnpm run build:config && pnpm run build:theme",
		"build": "pnpm run build:stencil && pnpm run format && pnpm run docs:copy:readme",
		"build:stencil": "stencil build --docs",
		"build:config": "tsx scripts/build-config.ts",
		"build:theme": "tsx scripts/build-theme-scss.ts",
		"clean": "rm -rf dist loader www",
		"clean:full": "rm -rf node_modules && pnpm run clean",
		"clean:icons": "rm -rf src/components/ontario-icon/ontario-icon-*.tsx",
		"docs:assemble": "tsx scripts/build-documentation-bundle.ts",
		"docs:copy:readme": "tsx ../../scripts/documentation-helper.ts -f readme.md -p \"---\nslug: /\n---\n\" -df ../docusaurus/docs/01-readme.md",
		"format": "prettier --write .",
		"generate": "stencil generate",
		"generate:icons": "node src/components/ontario-icon/scripts/generate-icons.cjs",
		"start": "stencil build --dev --watch --serve",
		"pretest": "pnpm run build:config",
		"test": "stencil test --spec --e2e",
		"test.watch": "stencil test --spec --e2e --watchAll",
		"pretest:e2e": "pnpm run build:config",
		"test:e2e": "playwright test",
		"test:e2e:docker": "../../scripts/docker-compose.sh run --rm stencil-e2e-runner",
		"pretest:unit": "pnpm run build:config",
		"test:unit": "stencil test --spec"
	},
	"dependencies": {
		"@ongov/ontario-design-system-design-tokens": "workspace:*",
		"@ongov/ontario-design-system-global-styles": "workspace:*",
		"@stencil/core": "^4.33.1",
		"dotenv": "^16.4.5",
		"uuid": "^11.0.5"
	},
	"devDependencies": {
		"@playwright/test": "1.55.0",
		"@stencil/angular-output-target": "^0.10.1",
		"@stencil/playwright": "^0.2.1",
		"@stencil/react-output-target": "^1.4.0",
		"@stencil/sass": "^3.2.2",
		"@types/jest": "^29.5.3",
		"@types/mock-fs": "^4.13.4",
		"@types/node": "^22.12.0",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@types/uuid": "^10.0.0",
		"babel-jest": "^29.7.0",
		"copyfiles": "^2.4.1",
		"fs-extra": "^11.2.0",
		"glob": "^11.0.0",
		"jest": "^29.7.0",
		"jest-cli": "^29.7.0",
		"jest-junit": "^16.0.0",
		"jest-svg-transformer": "^1.0.0",
		"json-loader": "^0.5.7",
		"jszip": "^3.10.1",
		"mock-fs": "^5.5.0",
		"prettier": "^3.4.2",
		"rollup-plugin-dotenv": "^0.5.1",
		"sass-embedded": "^1.89.2",
		"shx": "^0.4.0",
		"stencil-inline-svg": "^1.1.0",
		"svgson": "^5.3.1",
		"tsx": "^4.19.4",
		"typescript": "^5.6.3",
		"validate-color": "^2.2.4"
	},
	"publishConfig": {
		"access": "public"
	},
	"authors": [
		{
			"name": "Scott Morris",
			"email": "Scott.Morris@ontario.ca"
		},
		{
			"name": "Elsa Ng",
			"email": "Elsa.Ng@ontario.ca"
		},
		{
			"name": "Erin Carrieres",
			"email": "Erin.Carrieres@ontario.ca"
		},
		{
			"name": "Meaghan Carrieres",
			"email": "Meaghan.Carrieres@ontario.ca"
		},
		{
			"name": "Nakib Momin",
			"email": "nakib.momin@ontario.ca"
		}
	],
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs"
}
